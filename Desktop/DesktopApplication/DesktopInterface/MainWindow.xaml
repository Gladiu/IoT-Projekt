<Window x:Class="DesktopInterface.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DesktopInterface"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="WindowLoaded">
    <Grid>
        <Label Name="temperatureLabel"  Content="Label" HorizontalAlignment="Left" Margin="10,137,0,0" VerticalAlignment="Top" RenderTransformOrigin="-1.79,-1.424"/>
        <Label x:Name="humidityLabel"  Content="Label" HorizontalAlignment="Left" Margin="10,191,0,0" VerticalAlignment="Top" RenderTransformOrigin="-1.79,-1.424"/>
        <DataGrid Name="dataGrid" AutoGenerateColumns="False" >
            <DataGrid.Columns>
                <DataGridTextColumn Header="Name"  Binding="{Binding name}"/>
                <DataGridTextColumn Header="Unit" Binding="{Binding defaultUnit}" />
                <DataGridTextColumn Header="Value" Binding="{Binding value}" />
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
