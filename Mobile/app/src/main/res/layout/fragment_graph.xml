<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ap="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GraphViewModel"
    android:id="@+id/frameLayout3">


        <Button
            android:id="@+id/changeUnitsButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="120dp"
            android:text="Change Units"
            ap:layout_constraintBottom_toBottomOf="parent"
            ap:layout_constraintEnd_toEndOf="parent"
            ap:layout_constraintHorizontal_bias="0.498"
            ap:layout_constraintStart_toStartOf="parent"
            ap:layout_constraintTop_toBottomOf="@+id/graphView"
            ap:layout_constraintVertical_bias="0.003"
            tools:layout_conversion_absoluteHeight="48dp"
            tools:layout_conversion_absoluteWidth="98dp" />

        <com.jjoe64.graphview.GraphView
            android:id="@+id/graphView"
            android:layout_width="317dp"
            android:layout_height="213dp"
            android:layout_marginTop="56dp"
            ap:layout_constraintEnd_toEndOf="parent"
            ap:layout_constraintHorizontal_bias="0.496"
            ap:layout_constraintStart_toStartOf="parent"
            ap:layout_constraintTop_toTopOf="parent"
            tools:layout_conversion_absoluteHeight="200dp"
            tools:layout_conversion_absoluteWidth="411dp" />


</androidx.constraintlayout.widget.ConstraintLayout>
